#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/songjunwei/Documents/study/study/vue/vue-test-source/Learn-vue-source-notes/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/songjunwei/Documents/study/study/vue/vue-test-source/Learn-vue-source-notes/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../registry.npmmirror.com+webpack-dev-server@3.11.3_webpack@4.46.0/node_modules/webpack-dev-server/bin/webpack-dev-server.js" "$@"
else
  exec node  "$basedir/../../../../../../registry.npmmirror.com+webpack-dev-server@3.11.3_webpack@4.46.0/node_modules/webpack-dev-server/bin/webpack-dev-server.js" "$@"
fi
